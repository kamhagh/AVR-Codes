#include <avr/io.h>
#include <util/delay.h>

int main(void)
{
    DDRD |= 1 << 4;
	
    TCCR1A|=(1<<COM1B1)|(1<<WGM11);
    TCCR1B |= 1 << WGM13 | 1 << WGM12 | 1 << CS11 | 1 < CS10;
    ICR1=4999

  while(1)
   {
     OCR1A = 316
     _delay_ms(600);
     OCR1A = 97;
     _delay_ms(600);
     OCR1A = 500;
     _delay_ms(600);
   }

}
